<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/receiving_item"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/backgroundE8E8E8"
    tools:context=".Main.Receiving.Tab.Receiving_item">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="2">
        <com.github.mikephil.charting.charts.HorizontalBarChart
            android:id="@+id/receivingItem_HorbarChart"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:foreground="?android:attr/selectableItemBackground"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/card_margin"
        android:paddingLeft="@dimen/card_margin"
        android:paddingRight="@dimen/card_margin"
        android:paddingTop="@dimen/card_margin"
        android:layout_weight="1">

        <com.example.james.rms.CommonProfile.Library.AnimatedExpandableListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/receiving_item_listView"></com.example.james.rms.CommonProfile.Library.AnimatedExpandableListView>
        <!---->
        <!--<ListView-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="match_parent"-->
            <!--android:id="@+id/receiving_item_listView"></ListView>-->
    </LinearLayout>

</LinearLayout>
