<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/product_pool"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/backgroundE8E8E8"
    tools:context="com.example.james.rms.ProductPool.Tab.ProductDetail">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/card_margin"
        android:paddingTop="@dimen/card_margin"
        android:paddingLeft="@dimen/card_margin"
        android:paddingRight="@dimen/card_margin"
        android:layout_weight="1">
        <com.example.james.rms.CommonProfile.Library.AnimatedExpandableListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/product_listView"></com.example.james.rms.CommonProfile.Library.AnimatedExpandableListView>
    </LinearLayout>
</LinearLayout>
